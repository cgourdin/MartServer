    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-install-plugin</artifactId>
                <version>2.4</version>
                <executions>
                    <execution>
                        <id>Clouddesigner-occi</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <groupId>Clouddesigner</groupId>
                            <artifactId>org.occiware.clouddesigner.occi</artifactId>
                            <version>0.1.0-SNAPSHOT</version>
                            <packaging>jar</packaging>
                            <file>${basedir}/lib/org.occiware.clouddesigner.occi-0.1.0-SNAPSHOT.jar</file>
                        </configuration>
                    </execution>
                    <execution>
                        <id>Clouddesigner-occi-infrastructure</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <groupId>Clouddesigner</groupId>
                            <artifactId>org.occiware.clouddesigner.occi.infrastructure</artifactId>
                            <version>0.1.0-SNAPSHOT</version>
                            <packaging>jar</packaging>
                            <file>${basedir}/lib/org.occiware.clouddesigner.occi.infrastructure-0.1.0-SNAPSHOT.jar
                            </file>
                        </configuration>
                    </execution>
                    <execution>
                        <id>eclipse-ocl</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <groupId>org.eclipse.ocl</groupId>
                            <artifactId>org.eclipse.ocl.pivot</artifactId>
                            <version>1.0.1.v20150908-1239</version>
                            <packaging>jar</packaging>
                            <file>${basedir}/lib/org.eclipse.ocl.pivot-1.0.1.v20150908-1239.jar</file>
                        </configuration>
                    </execution>
                    <execution>
                        <id>eclipse-ocl-common</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <groupId>org.eclipse.ocl</groupId>
                            <artifactId>org.eclipse.ocl.common</artifactId>
                            <version>1.3.0.v20150519-0914</version>
                            <packaging>jar</packaging>
                            <file>${basedir}/lib/org.eclipse.ocl.common-1.3.0.v20150519-0914.jar</file>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>2.6</version>
                <configuration>
                    <archive>
                        <manifest>
                            <addClasspath>true</addClasspath>
                            <mainClass>org.occiware.mart.server.MartServer</mainClass>
                            <classpathPrefix>lib/</classpathPrefix>
                        </manifest>
                    </archive>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <version>1.5.0</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>java</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <mainClass>org.occiware.mart.server.MartServer</mainClass>
                </configuration>
            </plugin>
        </plugins>
    </build>


